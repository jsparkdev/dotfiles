{
  "autosave": "on_focus_change",
  "buffer_font_family": "JetBrains Mono",
  "buffer_font_weight": 600,
  "confirm_quit": true,
  "inline_completions": {
    "disabled_globs": [".env", ".md", ".mdx"]
  },
  "scrollbar": {
    "show": "always"
  },
  "tab_bar": {
    "show_nav_history_buttons": false
  },
  "tabs": {
    "git_status": true
  },
  "toolbar": {
    "quick_actions": false
  },
  "languages": {
    "TypeScript": {
      "code_actions_on_format": {
        "source.organizeImports": true,
        "source.fixAll": true
      }
    },
    "TSX": {
      "code_actions_on_format": {
        "source.organizeImports": true,
        "source.fixAll": true
      }
    },
    "Astro": {
      "code_actions_on_format": {
        "source.organizeImports": true,
        "source.fixAll": true,
        "source.fixAll.eslint": true
      }
    },
    "JavaScript": {
      "code_actions_on_format": {
        "source.organizeImports": true,
        "source.fixAll.eslint": true
      }
    },
    "JSX": {
      "code_actions_on_format": {
        "source.organizeImports": true,
        "source.fixAll.eslint": true
      }
    }
  },
  "lsp": {
    "eslint": {
      "settings": {
        "codeActionOnSave": {
          "rules": ["import/order"]
        }
      }
    }
  },
  "always_treat_brackets_as_autoclosed": true,
  "indent_guides": {
    "coloring": "disabled",
    "background_coloring": "indent_aware"
  },
  "show_whitespaces": "none",
  "soft_wrap": "editor_width",
  "tab_size": 2,
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "terminal": {
    "font_size": 12,
    "line_height": "standard",
    "blinking": "on",
    "working_directory": {
      "always": {
        "directory": "~/code"
      }
    }
  },
  "project_panel": {
    "indent_size": 12,
    "scrollbar": {
      "show": "never"
    }
  },
  "assistant": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-20240620"
    },
    "version": "2"
  },
  "unnecessary_code_fade": 0.7,
  "base_keymap": "VSCode",
  "ui_font_size": 13,
  "ui_font_family": "JetBrains Mono",
  "ui_font_weight": 700,
  "theme": {
    "mode": "dark",
    "light": "Andromeda",
    "dark": "Andromeda"
  },
  "features": {
    "inline_completion_provider": "supermaven"
  }
}
